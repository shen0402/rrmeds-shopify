    <script>

      if ($(window).scrollTop() > 100 ){
        $('.nav-brand-img').addClass('nav-brand-img-scrolling');
        $('.fixed-nav-wrapper').addClass('fixed-nav-wrapper-scrolling');
        $('.jc-fixed-nav-logo').addClass('jc-logo-shrink');
        $('.jc-fixed-nav-brand').addClass('jc-nav-link-scroll');
      }


    $(window).scroll(function() {
      if ($(window).scrollTop() > 100 ){
        $('.nav-brand-img').addClass('nav-brand-img-scrolling');
        $('.fixed-nav-wrapper').addClass('fixed-nav-wrapper-scrolling');
        
        $('.jc-fixed-nav-logo').addClass('jc-logo-shrink');
       // $('.nav-link').addClass('nav-link-scroll');
        $('.jc-fixed-nav-brand').addClass('jc-nav-link-scroll');
        } else {
          $('.nav-brand-img').removeClass('nav-brand-img-scrolling');
          $('.fixed-nav-wrapper').removeClass('fixed-nav-wrapper-scrolling');
        $('.jc-fixed-nav-logo').removeClass('jc-logo-shrink');
       // $('.nav-link').removeClass('nav-link-scroll');
        $('.jc-fixed-nav-brand').removeClass('jc-nav-link-scroll');
      }; 
    });
  


  </script>



