      </div>
    </div>
    <div class="jc-fixed-nav-overlay"></div>
    {% if template == 'index' %}
      <div class="jc-fixed-nav-overlay-alt"></div>
    {% endif %}
  </div>
</div>

{% schema %}
  {
    "name": "Header" ,
      "settings": [
        {
           "type": "richtext",
           "id": "banner_text",
           "default": "<p>FREE SHIPPING ON ALL DOMESTIC ORDERS!</p>",
           "label": "Banner Text"
        }
    ]
  }
{% endschema %}








