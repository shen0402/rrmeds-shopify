{% comment %}
<div class="jc-fixed-bar-mobile-wrapper">
  <div class="jc-fixed-bar-link"><strong>{{section.settings.banner_text}}</strong></div>
</div>
{% endcomment %}
<div class="jc-fixed-wrapper">
  
  <div class="jc-fixed-bar-wrapper">
    <div class="jc-container-large">
      <div class="jc-fixed-bar-row w-row">
        <div class="jc-fixed-bar-col jc-fixed-bar-col-social w-col w-col-3">
          <div class="jc-fixed-bar-social-row">
            <a href="https://www.facebook.com/rrmedicinals/" class="jc-fixed-bar-social-link w-inline-block" target="_blank"><img src="https://cdn.shopify.com/s/files/1/0107/7431/5072/files/RR-reskin-nav_AST_facebook_icon.svg?v=1658163170" alt="" class="jc-fixed-bar-social-img"></a>
            <a href="https://www.instagram.com/rrmedicinals/" class="jc-fixed-bar-social-link w-inline-block" target="_blank"><img src="https://cdn.shopify.com/s/files/1/0107/7431/5072/files/RR-reskin-nav_AST_IG_icon.svg?v=1658163169" alt="" class="jc-fixed-bar-social-img"></a>
            <a href="mailto:contact@rrmeds.com" class="jc-fixed-bar-social-link w-inline-block"><img src="https://cdn.shopify.com/s/files/1/0107/7431/5072/files/RR-reskin-nav_AST_email_icon.svg?v=1658163169" alt="" class="jc-fixed-bar-social-img"></a>
          </div>
        </div>
        <div class="jc-fixed-bar-col-center w-col w-col-6"><div class="jc-fixed-bar-link">{{section.settings.banner_text}}</div></div>
        <div class="jc-fixed-bar-col-right w-col w-col-3">
          <div class="jc-fixed-bar-flex-row">

            {% if customer %}

            <div class="jc-fixed-bar-flex-link-wrapper">
              <a href="/account" class="jc-fixed-bar-flex-link w-inline-block">
                <div>Hi, {{ customer.first_name }}!</div>
              </a>
              <div data-hover="1" data-delay="0" class="jc-fixed-bar-dropdown w-dropdown">
                <div class="jc-fixed-bar-dropdown-toggle w-dropdown-toggle"><img src="https://cdn.shopify.com/s/files/1/0107/7431/5072/files/dropdown-triangle-alt-min.png" alt="Down Arrow" class="jc-fixed-bar-flex-link-img" /></div>
                <nav class="jc-fixed-bar-dropdown-menu w-dropdown-list">
                  <a href="/account" class="jc-fixed-bar-dropdown-link w-dropdown-link">Order History</a>
                  <a href="/account?rewards" class="jc-fixed-bar-dropdown-link w-dropdown-link">Rewards Points</a>
                  <a href="https://www.rrmeds.com/tools/checkout/api/manage/subscription/app" class="jc-fixed-bar-dropdown-link w-dropdown-link">Auto Shipments</a>
                  <a href="/account/logout" class="jc-fixed-bar-dropdown-link w-dropdown-link">Log Out</a>
                </nav>
              </div>
            </div>

            {% else %}
              <a href="/account/login" class="jc-fixed-bar-flex-link w-inline-block">
                <div>
                  Sign In
                </div>
              </a>
            {% endif %}
              
            
            <a href="/cart" class="jc-fixed-bar-cart-link w-inline-block">
              <img src="https://cdn.shopify.com/s/files/1/0107/7431/5072/files/RR-reskin-nav_AST_shopping_bag_icon.svg?v=1658163170" alt="" class="jc-fixed-bar-cart-img">
              <div id="nav-cart-quantity" class="cart-count-number">{{cart.item_count}}</div>
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="jc-fixed-nav-wrapper">
    <div data-collapse="medium" data-animation="default" data-duration="400" class="jc-fixed-navbar w-nav">
      <div class="jc-fixed-nav-container w-container">
        <div class="jc-fixed-nav-flex-row"><a href="/" class="jc-fixed-nav-brand w-nav-brand">
          <img src="https://cdn.shopify.com/s/files/1/0107/7431/5072/files/RR-reskin-nav_AST_R_R_Logo.svg?v=1658163169" alt="R + R Medicinals. Trusted Hemp. Real Results." class="jc-fixed-nav-logo">
        </a>
          <nav role="navigation" class="jc-nav-menu w-nav-menu">

