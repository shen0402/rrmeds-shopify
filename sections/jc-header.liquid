                <a href="/pages/rewards" class="jc-nav-dropdown-link jc-nav-dropdown-link-last w-dropdown-link">Loyalty Discounts</a>
                </nav>
            </div>

            <div data-hover="1" data-delay="0" class="w-dropdown">
              <div class="jc-nav-link jc-nav-link-dropdown w-dropdown-toggle" id="jc-nav-link-hold-active-2">
                <div class="jc-nav-dropdown-text">Contact</div>
                <div class="jc-nav-dropdown-icon w-icon-dropdown-toggle"></div>
                <a href="/pages/contact" class="jc-nav-overlay-link w-inline-block"></a>
